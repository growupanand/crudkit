{
  "name": "crudkit",
  "version": "0.1.0",
  "description": "Automate CRUD operations with Drizzle, tRPC, and React Query",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": ["dist", "README.md"],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
