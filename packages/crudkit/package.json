    "zod": ">=3.0.0"
  },
  "devDependencies": {
    "@trpc/client": "11.0.2",
    "@trpc/react-query": "11.0.2",
    "@trpc/server": "11.0.2",
    "@types/node": "22.14.0",
    "@types/react": "19.1.0",
    "drizzle-orm": "0.41.0",
    "drizzle-zod": "0.5.1",
    "react": "19.1.0",
    "tsup": "8.4.0",
    "typescript": "5.8.3",
